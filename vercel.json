{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://bmw-backend-production.up.railway.app/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://bmw-backend-production.up.railway.app/uploads/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
